    "jest": "^27.5.1",
    "less": "^4.1.2",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.4",
    "vue-eslint-parser": "^8.3.0",
    "vue-jest": "^3.0.7",
    "vue-router": "^4.0.12",
